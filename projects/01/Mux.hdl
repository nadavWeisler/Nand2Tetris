/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
		Nand(a=b, b=sel, out=y);
		Not(in=sel, out=notSel);
		Nand(a=a, b=notSel, out=z);
		Nand(a=y, b=z, out=out);
}