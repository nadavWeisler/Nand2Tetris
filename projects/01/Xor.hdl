/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
		Not(in=a, out=notA);
		Not(in=b, out=notB);
		And(a=a, b=b, out=andRes);
		And(a=notA, b=notB, out=andNotRes);
		Or(a=andNotRes, b=andRes, out=orRes);
		Not(in=orRes, out=out);
}